  <data name="ListaMovimentoCotaSite" xml:space="preserve">
    <value>SELECT        mov.codfundo, mov.data, mov.vlr_min_aplic_ini, mov.vlr_min_aplic_adic, mov.vlr_min_resgate, mov.vlr_min_aplic, mov.identificador, taxa.cobra_taxa_perform, 
                         taxa.taxa_entrada, fundos.prazo_conv_resg, fundos.prazo_pgto_resg, fundos.perfil_cota, rent.pl, taxa.taxa_fixa, mov.DataHora,taxa.taxa_perform
FROM            DBA.fundos_mov_cota mov, DBA.taxa_adm taxa, DBA.fundos fundos, DBA.fundos_dia rent
WHERE        mov.codfundo = taxa.codfundo AND taxa.codfundo = fundos.codfundo AND fundos.codfundo = rent.codfundo AND (fundos.codfundo = '{0}') AND 
                         (mov.data IN
                             (SELECT        MAX(data) AS Expr1
                               FROM            DBA.fundos_mov_cota
                               WHERE        (codfundo = '{0}'))) AND (rent.data IN
                             (SELECT        MAX(data) AS Expr1
                               FROM            DBA.fundos_dia
                               WHERE        (codfundo = '{0}')))</value>
  </data>
  <data name="ListaFundosSite" xml:space="preserve">
    <value>select 
a.codfundo as idCodigoAnbima
,a.fantasia as dsNomeProduto
,cnpj as CPFCNPJ
,classeCVM.opcoes_status as categoria
,tipoInvestidor.opcoes_status as dsClienteAlvo
,c.fantasia as Administrador
,b.fantasia as Gestor
,tributacao.opcoes_status as Tributacao
,a.codtipo as idTipoAmbima
,a.aberto as stPermitirNovasAplicacoes
,focoAtuacao.opcoes_status as FocoAtuacao
,a.dataIni
,a.datafim
,a.datainfo
from DBA.fundos a 
inner join DBA.instituicoes b
on a.gestor = b.codinst 
inner join DBA.instituicoes c
on a.codinst = c.codinst
left join DBA.fundos_status classeCVM
on a.codfundo = classeCVM.codfundo
and classeCVM.status = 'Classe CVM'
and classeCVM.datafim is null
left join DBA.fundos_status tipoInvestidor
on a.codfundo = tipoInvestidor.codfundo
and tipoInvestidor.status = 'Tipo de Investidor'
and tipoInvestidor.datafim is null
left join DBA.fundos_status tributacao
on a.codfundo = tributacao.codfundo
and tributacao.status = 'Tributação Alvo'
and tributacao.datafim is null
left join DBA.fundos_status focoAtuacao
on a.codfundo = focoAtuacao.codfundo
and focoAtuacao.status = 'Foco de Atuação'
and focoAtuacao.datafim is null
where a.codfundo = '{0}'
Group by
a.codfundo
,a.fantasia
,classeCVM.opcoes_status
,cnpj		
,tipoInvestidor.opcoes_status
,c.fantasia
,b.fantasia
,tributacao.opcoes_status
,a.codtipo
,a.aberto	
,focoAtuacao.opcoes_status
,a.dataIni
,a.datafim
,a.datainfo</value>
